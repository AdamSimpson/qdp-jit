all: manual.pdf quick_ref.pdf

manual.ps: manual.dvi
	dvips $< -o $@ 

manual.pdf: manual.dvi
	dvipdfm $<

manual.dvi: manual.tex
	latex $< 
	latex $<

quick_ref.ps: quick_ref.dvi
	dvips $< -o $@ 

quick_ref.pdf: quick_ref.dvi
	dvipdfm $<

quick_ref.dvi: quick_ref.tex
	latex $< 
	latex $<

clean:
	rm -rf *.toc *.lof *.dvi *.aux *~ *.pdf *.ps *.log
	rm -rf doxygen	
